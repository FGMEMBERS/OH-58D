<?xml version="1.0"?>

<PropertyList>

	<path>ALT.ac</path>

	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle100</object-name>
		<object-name>FT10000</object-name>
		<object-name>FT1000</object-name>
		<object-name>MB1000</object-name>
		<object-name>MB100</object-name>
		<object-name>MB10</object-name>
		<object-name>MB1</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Needle100</object-name>
		<object-name>axis</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.36</factor>
		<center>
			<x-m> 0.0028</x-m>
			<y-m>-0.0002</y-m>
			<z-m> 0.0012</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>FT1000</object-name>
		<property>/instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<scroll>20</scroll>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>FT10000</object-name>
		<property>/instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.00001</factor>
		<step>10000</step>
		<scroll>20</scroll>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>MB1000</object-name>
		<property>/instrumentation/altimeter/millibars</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>MB100</object-name>
		<property>/instrumentation/altimeter/millibars</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>MB10</object-name>
		<property>/instrumentation/altimeter/millibars</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>MB1</object-name>
		<property>/instrumentation/altimeter/millibars</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>